---
import { Icon } from 'astro-icon/components';

interface Props {
	class?: string;
	height?: number;
}

const { class: className, height = 120 } = Astro.props;

const width = height * 1.333_333_333_333_333_3;
---

<Icon
	class:list={['drop-shadow-xl', className]}
	height={height}
	name="logos/tsg-irlich-logo"
	width={width}
/>
